{
    "name": "panix/app",
    "description": "PIXELION CMS",
    "keywords": ["pixelion-cms"],
    "homepage": "https://pixelion.com.ua/",
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "issues": "https://github.com/andrtechno/app/issues?state=open",
        "source": "https://github.com/andrtechno/app"
    },
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "~2.0.6",
        "yiisoft/yii2-swiftmailer": "~2.0.0",
        "yiisoft/yii2-jui": "*",
        "yiisoft/yii2-authclient": "~2.1.0",
        "yiisoft/yii2-smarty": "^2.0@dev",
        "panix/engine": "*",
        "phpoffice/phpword": "v0.13.*",
        "panix/mod-stats": "*",
        "panix/mod-delivery": "*",
        "panix/mod-forum": "*",
        "yiisoft/yii2-bootstrap4": "^1.0.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
        "yiisoft/yii2-faker": "~2.0.0",
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1"
    },
    "config": {
        "process-timeout": 1800

    },
    "scripts": {
        "post-install-cmd": [
            "yii\\composer\\Installer::postInstall"
        ],
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject",
            "yii\\composer\\Installer::postInstall"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "runtime": "0777",
                    "web/assets": "0777",
                    "yii": "0755"
                }
            ]
        },
        "yii\\composer\\Installer::postInstall": {
            "generateCookieValidationKey": [
                "config/web.php"
            ]
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
