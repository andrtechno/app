        "vakata/jstree": "dev-master",
        "panix/wgt-jstree": "*",
        "panix/wgt-tinymce": "*",
        "panix/mod-shop": "dev-master",
        "panix/mod-pages": "dev-master",
        "pistol88/yii2-seo": "*",
        "vlaim/yii2-file-upload": "dev-master",
        "trntv/yii2-file-kit": "dev-master",
        "salopot/yii2-attach-image": "dev-master",
        "panix/mod-user": "dev-master",
        "panix/mod-admin": "*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0.0",
        "yiisoft/yii2-gii": "~2.0.0",
